<!DOCTYPE html>
<% provide(:title,'Tabla de Posts')%>


<div class="formularios" id="tablas">
<h1>Posts</h1>

    <% @posts.each do |post| %>
        <div class="post">
            <div class="header">
                <h4><%=  post.user.name %></h4>
            </div>
            <div class="body">
                <p class="card-text"><%= post.content %></p>
            </div> 
        </div>

    <% end %>
    
</div>